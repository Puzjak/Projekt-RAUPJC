@model System.Collections.Generic.IList<RAUPJC_Projekt.Core.ServiceLogic.Service>

<body>
<div class="container">
    <table class="table table-condensed table-responsive">
        <thead>
        <tr>
            <th>Usluga</th>
            <th>Trajanje(min)</th>
            <th>Ukloni uslugu</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var service in Model)
        {
            <tr>
                <td>@service.Name</td>
                <td>@service.Duration</td>
                <td><a asp-action="RemoveService" asp-controller="Administrator" asp-route-id="@service.ServiceId" class="btn btn-danger">
                    Ukloni uslugu
                </a></td>
            </tr>
        }
        </tbody>
    </table>
</div>
</body>